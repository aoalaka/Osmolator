<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.aoalaka.android.osmolator">

    <application
        android:allowBackup="true"
        android:icon="@drawable/solution_logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_action_lab"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ResultActivity"
            android:label="@string/result_page_label"
            android:screenOrientation="portrait">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.aoalaka.android.osmolator.MainActivity" />
        </activity>
        <activity
            android:name=".DeveloperNoteActivity"
            android:label="@string/developer_note"
            android:screenOrientation="portrait">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.aoalaka.android.osmolator.MainActivity" />
        </activity>
    </application>

</manifest>